 #include <stdio.h>

 int main()
 {

    printf("Enter integers (you will be prompted 4 times): \n");
    int k = 0;
    scanf("%d", &k);

    int min=k, max=k;
    for(int i = 1; i<=3; i++){
    scanf("%d", &k);

    if (k > max){
     max = k;
    }

    if (k < min){
     min = k;
    }
    }

    printf("Max     :   %d\n", max);
    printf("Min     :   %d\n", min);
    
    return 0;
 }